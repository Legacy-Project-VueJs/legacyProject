{"ast":null,"code":"import axios from \"axios\";\nimport CartBadge from \"./CartBadge.vue\";\nimport FoodDetails from \"./FoodDetails.vue\";\nimport FooterSectionContainer from \"./FooterSectionContainer.vue\";\nexport default {\n  name: \"HomePage\",\n  components: {\n    CartBadge,\n    FoodDetails,\n    FooterSectionContainer\n  },\n  data() {\n    return {\n      damy: null,\n      selectedFood: null,\n      searchText: ''\n    };\n  },\n  mounted() {\n    this.loadFoods();\n  },\n  methods: {\n    loadFoods() {\n      axios.get(\"http://localhost:5000/api/Foods/getall\").then(res => this.damy = res.data).catch(err => console.log(err));\n    },\n    viewFoodDetails(foodItem) {\n      this.selectedFood = foodItem; // set the selected food item\n    },\n\n    searchFood() {\n      if (this.searchText.trim() !== '') {\n        axios.get(`http://localhost:5000/api/Foods/${this.searchText}`).then(res => this.damy = res.data).catch(err => console.log(err));\n      } else {\n        this.loadFoods();\n      }\n    },\n    goBack() {\n      this.selectedFood = null;\n    }\n  }\n};","map":{"version":3,"names":["axios","CartBadge","FoodDetails","FooterSectionContainer","name","components","data","damy","selectedFood","searchText","mounted","loadFoods","methods","get","then","res","catch","err","console","log","viewFoodDetails","foodItem","searchFood","trim","goBack"],"sources":["C:\\Users\\nabil\\OneDrive\\Desktop\\10\\legacyProject\\frontEnd\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n  <section v-if=\"!selectedFood\">\r\n  <v-container>\r\n    <v-row class=\"d-flex align-items-center justify-content-between\">\r\n      <v-col cols=\"6\">\r\n        <div class=\"hero_content\">\r\n          <div class=\"hero_subtitle d-flex align-items-center\">\r\n            <p> Know Before You Go</p>\r\n            <img src=\"https://media.istockphoto.com/id/1276365565/vector/food-delivery-icon-vector-for-graphic-design-logo-web-site-social-media-mobile-app-ui.jpg?s=612x612&w=0&k=20&c=VAC6PLrWW-edpUIq_YaEbdcV4aU7lEdMI_UqJKIEkik=\" alt=\"img\" />\r\n          </div>\r\n          <h1>\r\n            Fast && Yummy\r\n          </h1>\r\n          <p>\r\n           Welcome to our food delivery website, where we offer a hassle-free and convenient way for you to enjoy delicious meals in the comfort of your own home. We understand the importance of good food and convenience, which is why we offer a wide range of cuisines from different restaurants, all at the click of a button.\r\n          </p>\r\n        </div>\r\n      </v-col>\r\n      <v-col cols=\"6\">\r\n      \r\n        <div class=\"hero-img-box mt-5\">\r\n          <img src=\"https://static.vecteezy.com/system/resources/previews/007/487/564/original/cute-pizza-icon-illustration-pizza-sticker-cartoon-logo-food-icon-concept-flat-cartoon-style-suitable-for-web-landing-page-banner-sticker-background-kawaii-pizza-vector.jpg\" alt=\"img\" />\r\n        </div>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</section>\r\n\r\n        <div class=\"search-bar\" v-if=\"!selectedFood\">\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"searchText\"\r\n            placeholder=\"Search for food...\"\r\n          />\r\n          <button @click=\"searchFood\">Search</button>\r\n        </div>\r\n     \r\n    <div class=\"container\" v-if=\"!selectedFood\">\r\n      <div class=\"containerr\">\r\n        <ul class=\"product-list\">\r\n          <li v-for=\"ele in damy\" :key=\"ele._id\" class=\"product-item\">\r\n            <div class=\"product-img\">\r\n              <img :src=\"ele.img\" />\r\n            </div>\r\n            <div class=\"product-info\">\r\n              <h3 class=\"product-title\">{{ ele.des }}</h3>\r\n              <p class=\"product-name\">{{ ele.name }}</p>\r\n              <p class=\"product-price\">$ {{ ele.price }}</p>\r\n              <div>\r\n                <CartBadge :product=\"ele\" />\r\n                <button @click=\"viewFoodDetails(ele)\">View Details</button>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else>\r\n      <button @click=\"goBack\">Back to Home</button>\r\n      <FoodDetails :foodItem=\"selectedFood\" />\r\n    </div>\r\n    <FooterSectionContainer />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport CartBadge from \"./CartBadge.vue\"\r\nimport FoodDetails from \"./FoodDetails.vue\";\r\nimport FooterSectionContainer from \"./FooterSectionContainer.vue\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n  \r\n  components:{CartBadge,FoodDetails,FooterSectionContainer},\r\n  data() {\r\n    return {\r\n      damy: null,\r\n      selectedFood: null,\r\n      searchText: ''\r\n    };\r\n  },\r\n  mounted() {\r\n    this.loadFoods();\r\n  },\r\n  methods: {\r\n    loadFoods() {\r\n      axios\r\n        .get(\"http://localhost:5000/api/Foods/getall\")\r\n        .then((res) => (this.damy = res.data))\r\n        .catch((err) => console.log(err));\r\n    },\r\n    viewFoodDetails(foodItem) {\r\n      this.selectedFood = foodItem; // set the selected food item\r\n    },\r\n    searchFood() {\r\n      if (this.searchText.trim() !== '') {\r\n        axios\r\n          .get(`http://localhost:5000/api/Foods/${this.searchText}`)\r\n          .then((res) => (this.damy = res.data))\r\n          .catch((err) => console.log(err));\r\n      } else {\r\n        this.loadFoods();\r\n      }\r\n    },\r\n    goBack() {\r\n      this.selectedFood = null; \r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.containerr {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 0 20px;\r\n}\r\n\r\n.product-list {\r\n  list-style: none;\r\n  margin: 0;\r\n  padding: 0;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.product-item {\r\n  flex: 1 0 30%;\r\n  margin: 20px;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.product-img img {\r\n  max-width: 100%;\r\n}\r\n\r\n.product-title {\r\n  margin: 10px 0;\r\n  font-size: 1.2em;\r\n}\r\n\r\n.product-price {\r\n  margin: 10px 0;\r\n  font-weight: bold;\r\n  font-size: 1.5em;\r\n}\r\n\r\n.add-to-cart-btn {\r\n  display: inline-block;\r\n  margin: 10px 0;\r\n  padding: 10px 20px;\r\n  border: none;\r\n  background-color: #0066cc;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  font-size: 1em;\r\n  font-weight: bold;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n  border-radius: 5px;\r\n}\r\n\r\n.add-to-cart-btn:hover {\r\n  background-color: #004080;\r\n}\r\n\r\n.hero_subtitle img{\r\n  width: 3.3rem ;\r\n  height: 2.3rem;\r\n}\r\n.hero_content h1{\r\n  font-size: 3.2rem;\r\n  font-weight: 600;\r\n  margin-top: 1rem;\r\n  margin-bottom: 2rem;\r\n  color: black;\r\n}\r\n\r\n.hero_content p {\r\n  font-size: 1.1rem;\r\n  color: black; \r\n  line-height: 2rem;\r\n}\r\n.hero-img-box img{\r\n  width: 100%;\r\n  height: 350px;\r\n  border-radius: 20px;\r\n  border: 1px solid crimson;\r\n  object-fit: cover;\r\n}\r\n.hero_content{\r\n  padding-top: 3.5rem;\r\n}\r\n.hero-img-box{\r\n  padding-top: 2rem;\r\n}\r\n.search-bar {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin-top: 3rem;\r\n}\r\n\r\n.search-bar input{\r\n  width: 60%;\r\n  height: 50px;\r\n  border-radius: 5px;\r\n  border: none;\r\n  padding: 0 10px;\r\n  font-size: 1.1rem;\r\n}\r\n\r\n.search-bar button {\r\n  width: 20%;\r\n  height: 50px;\r\n  margin-left: 1rem;\r\n  border-radius: 5px;\r\n  border: none;\r\n  background: crimson;\r\n  color: #fff;\r\n  font-size: 1.1rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.search-bar button:hover {\r\n  background: #fff;\r\n  color: crimson;\r\n  border: 2px solid crimson;\r\n}\r\n\r\n</style>"],"mappings":"AAoEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,iBAAgB;AACtC,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,sBAAqB,MAAO,8BAA8B;AAEjE,eAAe;EACbC,IAAI,EAAE,UAAU;EAEhBC,UAAU,EAAC;IAACJ,SAAS;IAACC,WAAW;IAACC;EAAsB,CAA