{"ast":null,"code":"import SignupForm from './components/SignupForm';\nimport LoginForm from './components/LoginForm';\nimport { auth } from './firebase/init.js';\nimport { signOut } from 'firebase/auth';\nexport default {\n  components: {\n    SignupForm,\n    LoginForm\n  },\n  data() {\n    return {\n      isLoggedIn: false,\n      showLogin: true,\n      displayName: ''\n    };\n  },\n  beforeUpdate() {\n    if (auth.currentUser) {\n      this.displayName = auth.currentUser.displayName;\n    }\n  },\n  methods: {\n    signOut() {\n      signOut(auth).then(() => {\n        // user signed-out\n        this.isLoggedIn = false;\n      });\n    }\n  }\n};","map":{"version":3,"names":["SignupForm","LoginForm","auth","signOut","components","data","isLoggedIn","showLogin","displayName","beforeUpdate","currentUser","methods","then"],"sources":["C:\\Users\\HP\\Desktop\\LegacyProject\\legacyProject\\frontEnd\\src\\App.vue"],"sourcesContent":["<template>\r\n\r\n  <div id=\"nav\" class=\"sticky_header\">\r\n    <div class=\"header\">\r\n      <div class=\"logo\">\r\n        <img src=\"https://t3.ftcdn.net/jpg/03/74/73/78/360_F_374737839_VsxlmnN7soqOOOFFzy5kfE9H6PFMCkPX.jpg\" alt=\"Logo\" />\r\n      </div>\r\n      <div class=\"mobile-menu\">&#9776;</div>\r\n      <ul class=\"menu\">\r\n        <li class=\"nav_item\">\r\n          <router-link to=\"/\" exact active-class=\"active_link\">Home</router-link>\r\n        </li>\r\n        <li class=\"nav_item\">\r\n          <router-link to=\"/about\" active-class=\"active_link\">About</router-link>\r\n        </li>\r\n        <li class=\"nav_item\">\r\n          <router-link to=\"/contact\" active-class=\"active_link\">Contact</router-link>\r\n        </li>\r\n        <li class=\"nav_item\">\r\n          <router-link to=\"/carte\" active-class=\"active_link\">\r\n            Cart\r\n            <span v-if=\"$store.state.cart.length > 0\">{{$store.state.cart.length}}</span>\r\n          </router-link>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n  <router-view />\r\n\r\n  <div v-if=\"!isLoggedIn\">\r\n    <!-- login -->\r\n    <template v-if=\"showLogin\">\r\n      <login-form @loggedIn=\"isLoggedIn = true\"/>\r\n      <p>No account yet? <span @click=\"showLogin=false\">Sign up</span> instead.</p>\r\n    </template>\r\n    <!-- or register -->\r\n    <template v-else>\r\n      <signup-form @loggedIn=\"isLoggedIn = true\"/>\r\n      <p>Already registered? <span @click=\"showLogin=true\">Login</span> instead.</p>\r\n    </template>\r\n  </div>\r\n  <!-- is logged in -->\r\n  <div v-else>\r\n    <h2>Welcome {{ displayName }}</h2>\r\n    <button @click=\"signOut\">Sign Out</button>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport SignupForm from './components/SignupForm'\r\nimport LoginForm from './components/LoginForm'\r\nimport { auth } from './firebase/init.js'\r\nimport { signOut } from 'firebase/auth'\r\n\r\nexport default {\r\n  components: { SignupForm, LoginForm },\r\n  data() {\r\n    return {\r\n      isLoggedIn: false,\r\n      showLogin: true,\r\n      displayName: ''\r\n    }\r\n  },\r\n  beforeUpdate() {\r\n    if (auth.currentUser) {\r\n      this.displayName = auth.currentUser.displayName\r\n    }\r\n  },\r\n  methods: {\r\n    signOut() {\r\n      signOut(auth)\r\n      .then(() => {\r\n        // user signed-out\r\n        this.isLoggedIn = false\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n\r\np,h2{text-align:center}span{text-decoration:underline;cursor:pointer;color:blue}\r\nform{display:flex;flex-direction:column;row-gap:16px;max-width:260px;margin:30px auto}\r\ninput,button{height:28px}input{padding-left:10px}button{width:fit-content;margin:auto}\r\n\r\n.header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  height: 80px;\r\n  line-height: 80px;\r\n  width: 100%;\r\n}\r\n\r\n.logo img {\r\n  width: 30%;\r\n}\r\n\r\n.menu {\r\n  display: flex;\r\n  list-style: none;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.nav_item {\r\n  margin-right: 20px;\r\n}\r\n\r\n.nav_item a {\r\n  color: black;\r\n  font-size: 1.1rem;\r\n  font-weight: 500;\r\n  text-decoration: none;\r\n}\r\n\r\n.nav_item a:hover {\r\n  color: inherit;\r\n}\r\n\r\n.nav_item a.active_link {\r\n  color: crimson;\r\n}\r\n\r\n.mobile-menu {\r\n  display: none;\r\n}\r\n\r\n@media only screen and (max-width: 768px) {\r\n  .menu {\r\n    display: none;\r\n    flex-direction: column;\r\n    position: absolute;\r\n    top: 80px;\r\n    left: 0;\r\n    width: 100%;\r\n    background: #ffe4b5;\r\n    padding: 20px;\r\n  }\r\n\r\n  .menu li {\r\n    margin-bottom: 10px;\r\n  }\r\n\r\n  .mobile-menu {\r\n    display: block;\r\n    cursor: pointer;\r\n  }\r\n}\r\n</style>\r\n\r\n\r\n"],"mappings":"AAkDA,OAAOA,UAAS,MAAO,yBAAwB;AAC/C,OAAOC,SAAQ,MAAO,wBAAuB;AAC7C,SAASC,IAAG,QAAS,oBAAmB;AACxC,SAASC,OAAM,QAAS,eAAc;AAEtC,eAAe;EACbC,UAAU,EAAE;IAAEJ,UAAU;IAAEC;EAAU,CAAC;EACrCI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK;MACjBC,SAAS,EAAE,IAAI;MACfC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,YAAYA,CAAA,EAAG;IACb,IAAIP,IAAI,CAACQ,WAAW,EAAE;MACpB,IAAI,CAACF,WAAU,GAAIN,IAAI,CAACQ,WAAW,CAACF,WAAU;IAChD;EACF,CAAC;EACDG,OAAO,EAAE;IACPR,OAAOA,CAAA,EAAG;MACRA,OAAO,CAACD,IAAI,EACXU,IAAI,CAAC,MAAM;QACV;QACA,IAAI,CAACN,UAAS,GAAI,KAAI;MACxB,CAAC;IACH;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}